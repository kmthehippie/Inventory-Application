<!DOCTYPE html>
<html lang="en">
<%- include('./partials/header') %>
<body>
  <h1></h1>
  <div class="container-fluid ">
    <div class="row">

        <div class="col-sm-3">
        <%- include('./partials/sidebar.ejs') %>
        </div>

        <div class="col-sm-9">
          <h1><%= title %></h1>
 
            <% sale_list.forEach(sale =>{ %>
                <p>Date: <small><%= sale.sale_date_formatted %></small> </p>
                  <p>ID: <a href="<%=sale.url%>"><%= sale._id %></a></p>   
                  <br/> 
            <% }) %>
        </div>
        
        <%- include("./partials/footer") %>
    </div>
</div>


</body>
</html>